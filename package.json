{
  "name": "ppspider",
  "author": "xiyuan_fengyu <xiyuan_fengyu@163.com> (https://github.com/xiyuan-fengyu)",
  "homepage": "github.com/xiyuan-fengyu/ppspider",
  "version": "0.1.23",
  "description": "spider build with puppeteer",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "auto build": "tsc",
    "国内用户安装依赖(windows)": "set PUPPETEER_DOWNLOAD_HOST=https://npm.taobao.org/mirrors/ & npm install",
    "国内用户安装依赖(unix)": "export PUPPETEER_DOWNLOAD_HOST=https://npm.taobao.org/mirrors/ & npm install",
    "generate README toc": "markdown-toc -i ./README.cn.md && markdown-toc -i ./README.md"
  },
  "keywords": [
    "spider",
    "crawler",
    "puppeteer",
    "headless",
    "chromium"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "later": "^1.2.0",
    "nedb": "^1.8.0",
    "puppeteer": "1.12.2",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "source-map-support": "^0.5.10",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jquery": "^3.3.29",
    "@types/nedb": "^1.8.5",
    "@types/node": "^11.9.4",
    "@types/puppeteer": "^1.12.1",
    "@types/request": "^2.48.1",
    "@types/socket.io": "^2.1.2",
    "markdown-toc": "^1.2.0"
  }
}
